{
  "extends": [
    "prettier",
    "prettier/react",
    "plugin:you-dont-need-lodash-underscore/compatible"
  ],
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": ["error"]
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "sourceType": "module"
  },
  "env": {
    "es6": true
  }
}
